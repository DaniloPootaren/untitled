{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "files": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files",
        "images"
      ]
    },
    "youtube": {
      "type": "component",
      "repeatable": true,
      "component": "links.youtube-link"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "featured": {
      "type": "boolean"
    },
    "article_sub_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article-sub-category.article-sub-category",
      "mappedBy": "articles"
    },
    "views": {
      "type": "integer",
      "default": 0
    }
  }
}
