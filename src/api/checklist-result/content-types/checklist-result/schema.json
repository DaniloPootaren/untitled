{
  "kind": "collectionType",
  "collectionName": "checklist_results",
  "info": {
    "singularName": "checklist-result",
    "pluralName": "checklist-results",
    "displayName": "Checklist Result",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "checklist_results"
    },
    "hospital": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::hospital.hospital"
    },
    "department": {
      "type": "string"
    },
    "ward": {
      "type": "string"
    },
    "visiting_team": {
      "type": "string"
    },
    "general_comment": {
      "type": "string"
    },
    "section": {
      "type": "dynamiczone",
      "components": [
        "checklist.free-text",
        "checklist.rating"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "submitted"
      ],
      "required": true
    },
    "checklist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::checklist.checklist"
    },
    "approved": {
      "type": "boolean",
      "default": false
    }
  }
}
