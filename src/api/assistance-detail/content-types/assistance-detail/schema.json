{
  "kind": "collectionType",
  "collectionName": "assistance_details",
  "info": {
    "singularName": "assistance-detail",
    "pluralName": "assistance-details",
    "displayName": "Assistance Detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "telephone": {
      "type": "string"
    },
    "assistance_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::assistance-category.assistance-category"
    },
    "working_hours": {
      "type": "component",
      "repeatable": false,
      "component": "assistance.working-hours"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "coordinates": {
      "type": "customField",
      "customField": "plugin::leaflet-plugin.leaflet-plugin",
      "required": true,
      "regex": "^-?\\d{1,2}\\.\\d{3,},\\s*-?\\d{1,3}\\.\\d{3,}$"
    }
  }
}
